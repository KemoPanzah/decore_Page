{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: MkDocs Serve",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": ".venv/Scripts/mkdocs.exe",
            "args": [
                "serve"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: MkDocs Build",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": ".venv/Scripts/mkdocs.exe",
            "args": [
                "build"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: MkDocs GH-Deploy",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": ".venv/Scripts/mkdocs.exe",
            "args": [
                "gh-deploy"
            ],
            "console": "integratedTerminal"
        }
    ]
}